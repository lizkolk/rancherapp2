
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<html>
<head>
</head>
<body>
    <h1>Liz's .NET Core 2.0 Web App Project for Rancher POC</h1>
    <img src="~/images/mall.png" />

    <div>
        <form method="POST">
            <h2>Rancher Get Test</h2>
            <div>
                <label for="urlInput">CONTAINER GET</label>
                <input type="text" name="httpUrl" id="urlInput" value=@TempData["Url"] class="form-control" />
                <br />
                <input type="submit" value="GET" asp-page-handler="HttpGet" class="btn btn-primary">
            </div>
        </form>
    </div>

    <div>
        <h2>Server Time</h2>
        Current time on the Rancher server is: @DateTime.Now.ToString()
    </div>

    <?xml version="1.0" standalone="no" ?>
    <svg width="250" height="350" version="1.1" xmlns="http://www.w3.org/2000/svg">
        <polygon points="50 160 55 180 70 180 60 190 65 205 50 195 35 205 40 190 30 180 45 180"
                 stroke="pink" fill="transparent" stroke-width="5" />
    </svg>

</body>
</html>